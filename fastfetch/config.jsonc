// Load with --load-config examples/2.jsonc
// Note that you must replace the image path to an existing image to display it.

{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "padding": {
            "top": 2
        }
    },
    "display": {
        "separator": " ➜  "
    },
    "modules": [
        {
            "type": "custom",
            "format": "┌────────────────────────────────────────────────────────────┐",
            "outputColor": "90"
        },
        {
            "type": "title",
            "keyWidth": 10
        },
        {
            "type": "custom",
            "format": "└────────────────────────────────────────────────────────────┘",
            "outputColor": "90"
        },
        {
            "type": "custom",
            "format": " {#90}  {#31}  {#32}  {#33}  {#34}  {#35}  {#36}  {#37}  {#38}  {#39}       {#38}  {#37}  {#36}  {#35}  {#34}  {#33}  {#32}  {#31}  {#90}"
        },
        
        {
            "type": "os",
            "key": " OS",
            "keyColor": "35"
        },
        {
            "type": "kernel",
            "key": "  ├ kernel",
            "keyColor": "35"
        },
        {
            "type": "packages",
            "key": "  ├󰏖 pkg   ",
            "keyColor": "35"
        },
        {
            "type": "shell",
            "key": "  └ shell ",
            "keyColor": "35"
        },
        "break",
        {
            "type": "wm",
            "key": " WM",
            "keyColor": "31"
        },
        // {
        //     "type": "lm",
        //     "key": "  ├󰧨 lm    ",
        //     "keyColor": "31"
        // },
        {
            "type": "wmtheme",
            "key": "  ├󰉼 wmtheme",
            "keyColor": "31"
        },
        {
            "type": "icons",
            "key": "  ├󰀻 icons ",
            "keyColor": "31"
        },
        {
            "type": "cursor",
            "key": "  ├ cursor",
            "keyColor": "31", 
        },
        {
            "type": "terminal",
            "key": "  ├ term  ",
            "keyColor": "31"
        },
        {
            "type": "terminalfont",
            "key": "  └ font  ",
            "keyColor": "31", 
        },
        {
            "type": "wallpaper",
            "key": "  └󰸉 wallpaper",
            "keyColor": "31"
        },
        "break",
        {
            "type": "host",
            "key": "󰌢 PC",
            "keyColor": "33"
        },
        {
            "type": "cpu",
            "key": "  ├ cpu   ",
            "keyColor": "33"
        },
        {
            "type": "gpu",
            "key": "  ├󰢮 gpu   ",
            "keyColor": "33"
        },
        {
            "type": "disk",
            "key": "  ├ disk  ",
            "keyColor": "33"
        },
        {
            "type": "memory",
            "key": "  ├ mem   ",
            "keyColor": "33"
        },
        {
            "type": "swap",
            "key": "  ├󰓡 swap  ",
            "keyColor": "33"
        },
        // {
        //     "type": "uptime",
        //     "key": "  ├󰅐",
        //     "keyColor": "33"
        // },
        {
            //"type": "monitor",
            "type": "display",
            "key": "  └󰍹 monitor",
            "keyColor": "33"
        },
        "break",
        {
            "type": "sound",
            "key": " SND",
            "keyColor": "cyan"
        },
        {
            "type": "player",
            "key": "  ├󰥠 player",
            "keyColor": "cyan"
        },
        {
            "type": "media",
            "key": "  └󰝚 media ",
            "format": "😄",
            "keyColor": "cyan"
        },
        "break",
        {
            "type": "custom",
            "format": " {#90}  {#31}  {#32}  {#33}  {#34}  {#35}  {#36}  {#37}  {#38}  {#39}       {#38}  {#37}  {#36}  {#35}  {#34}  {#33}  {#32}  {#31}  {#90}"
